<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="calculator.com.mycalculator">

    <application
        android:allowBackup="true"
        android:icon="@drawable/calc"
        android:label="@string/app_name"
        android:roundIcon="@drawable/calc"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="calculator.com.mycalculator.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="calculator.com.mycalculator.History_Activity"></activity>
    </application>

</manifest>